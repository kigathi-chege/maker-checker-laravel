{
  "name": "prismaticoder/maker-checker-laravel",
  "description": "A package for simplifying the integration of a maker-checker approval process to your Laravel application.",
  "keywords": [
    "php",
    "github",
    "laravel",
    "Open Source",
    "approvals",
    "admin",
    "maker-checker",
    "laravel 8",
    "laravel 9",
    "laravel 10"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Prismaticoder\\MakerChecker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Prismaticoder\\MakerChecker\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Jesutomiwa Salam",
      "email": "jesutomiwathefirst@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0",
    "laravel/serializable-closure": "^1.2.2"
  },
  "require-dev": {
    "orchestra/testbench": "7.x-dev"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Prismaticoder\\MakerChecker\\MakerCheckerServiceProvider"
      ]
    }
  }
}
